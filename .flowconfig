[ignore]
<PROJECT_ROOT>/.yarn/.*
<PROJECT_ROOT>/public/.*
<PROJECT_ROOT>/dist/.*
<PROJECT_ROOT>/node_modules/.*

<!-- Include Libraries -->
<!-- !<PROJECT_ROOT>/node_modules/react-dom
!<PROJECT_ROOT>/node_modules/flow-typed -->

[include]
<PROJECT_ROOT>/src/.*

[libs]

[lints]

[options]
module.name_mapper.extension='svg' -> '<PROJECT_ROOT>/src/SVGStub.js.flow'

experimental.component_syntax=true
experimental.component_syntax.hooklike_functions=true
experimental.react_rule=validateRefAccessDuringRender
experimental.react_rule=deepReadOnlyProps
experimental.react_rule=deepReadOnlyHookReturns
experimental.react_rule=rulesOfHooks
no_flowlib=false
all=true

[strict]